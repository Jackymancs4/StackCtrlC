{
  "manifest_version": 2,

  "name": "Stack Copy Button",
  "author": "MrMino",
  "description": "A copy button for Stack Overflow code boxes",
  "version": "1.1.1",

  "browser_action": {
    "default_icon": "icon32.png"
  },
  "icons": {
    "32":"icon32.png",
    "128":"icon128.png"
  },
  "permissions": [
    "*://stackoverflow.com/*",
    "clipboardWrite"
  ],

  "content_scripts": [ {
      "matches": ["*://*.stackoverflow.com/*"],
      "js": ["content.js"]
  } ]

}
