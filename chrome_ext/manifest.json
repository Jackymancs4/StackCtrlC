{
  "manifest_version": 2,

  "name": "Stack Copy Button",
  "description": "A copy button for Stack Overflow code boxes",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "32":"icon.png"
  },
  "permissions": [
    "*://stackoverflow.com/*",
    "clipboardWrite"
  ],

  "content_scripts": [ {
      "matches": ["*://*.stackoverflow.com/*"],
      "js": ["jquery-1.12.3.min.js", "copybox.min.js"]
  } ]

}
